{
    "name": "yii2dev/ar-softdelete",
    "description": "Provides support for ActiveRecord soft delete in Yii2",
    "keywords": ["yii2", "active", "record", "soft", "smart", "delete", "integrity"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Paul Klimov",
            "email": "klimov.paul@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "yiisoft/yii2": "^2.0.14"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "yii2dev\\ar\\softdelete\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "yii2dev\\ar\\softdelete\\tests\\": "tests"
        }
    },
    "archive": {
        "exclude": [
            ".php-cs-fixer.dist.php",
            "phpunit.xml.dist",
            "tests/",
            ".gitattributes",
            ".editorconfig",
            ".travis.yml"
        ]
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
